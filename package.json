{
    "name": "mm-node",
    "version": "1.0.0",
    "description": "Boilerplate for NodeJS application",
    "repository": {
        "type": "git",
        "url": "https://github.com/arun-awnics/mm-node.git"
    },
    "main": "index.js",
    "scripts": {
        "clean": "rm -rf dist",
        "compile": "npm run clean && babel src -d dist --copy-files",
        "serve": "npm run compile &&  node ./dist/index",
        "debug-run": "npm run compile && set DEBUG=*:* && node ./dist/index",
        "start": "nodemon --watch src --exec npm run serve",
        "test": "mocha --reporter spec dist/test/*.js"
    },
    "author": "Arun Gadag<arun.gadag@awnics.com>",
    "license": "MIT",
    "dependencies": {
        "async": "^2.5.0",
        "body-parser": "^1.17.2",
        "chai": "^4.1.2",
        "chai-http": "^3.0.0",
        "compression": "^1.7.0",
        "connect-mongo": "^1.3.2",
        "dotenv": "^4.0.0",
        "errorhandler": "^1.5.0",
        "expect.js": "^0.3.1",
        "express": "^4.15.4",
        "express-flash": "^0.0.2",
        "express-session": "^1.15.5",
        "express-status-monitor": "^1.0.0",
        "file-system": "^2.2.2",
        "jsonwebtoken": "^8.1.0",
        "lodash": "^4.17.4",
        "log4js": "^2.4.1",
        "lusca": "^1.5.1",
        "mocha": "^3.5.3",
        "mongodb": "^2.2.33",
        "mongoose": "^4.13.7",
        "multer": "^1.3.0",
        "mysql": "^2.15.0",
        "mysql2": "^1.4.2",
        "nodemailer": "^4.1.1",
        "passport": "0.4.0",
        "passport-jwt": "^3.0.1",
        "passport-local": "^1.0.0",
        "passport-oauth": "^1.0.0",
        "random-token-generator": "^1.0.2",
        "sequelize": "^4.22.5",
        "socket.io": "^2.0.4",
        "swagger-jsdoc": "^1.3.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.6.0",
        "chai": "^4.1.1",
        "mocha": "^3.5.0",
        "nodemon": "^1.12.1",
        "supertest": "^3.0.0"
    },
    "env": {
        "jasmine": true,
        "mocha": true,
        "node": true
    },
    "plugins": [
        "chai-friendly"
    ]
}