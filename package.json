{
    "name": "mm-node",
    "version": "1.0.0",
    "description": "Boilerplate for NodeJS application",
    "repository": {
        "type": "git",
        "url": "https://github.com/arun-awnics/mm-node.git"
    },
    "main": "index.js",
    "scripts": {
        "clean": "rm -rf dist",
        "compile": "npm run clean && babel src --out-dir dist --copy-files",
        "serve": "npm run compile && node -r dotenv/config ./dist/index dotenv_config_path=./.env.dev",
        "debug-run": "npm run compile && set DEBUG=*:* && nodemon ./dist/index",
        "start": "nodemon --watch src --exec npm run serve",
        "test": "mocha --reporter spec dist/test/*.js"
    },
    "author": "Arun Gadag<arun.gadag@awnics.com>",
    "license": "MIT",
    "dependencies": {
        "@google-cloud/storage": "^1.6.0",
        "actions-on-google": "^2.2.0",
        "async": "^2.5.0",
        "babel-runtime": "^6.26.0",
        "bcrypt": "^1.0.3",
        "body-parser": "^1.17.2",
        "chai": "^4.1.2",
        "chai-http": "^3.0.0",
        "compression": "^1.7.2",
        "connect-mongo": "^1.3.2",
        "cookie-parser": "^1.4.3",
        "dialogflow": "^0.6.0",
        "dialogflow-fulfillment": "^0.5.0",
        "dotenv": "^4.0.0",
        "email-templates": "^4.0.4",
        "errorhandler": "^1.5.0",
        "expect.js": "^0.3.1",
        "express": "^4.15.4",
        "express-flash": "^0.0.2",
        "express-session": "^1.15.5",
        "express-status-monitor": "^1.0.0",
        "file-system": "^2.2.2",
        "jsonwebtoken": "^8.1.1",
        "lodash": "^4.17.5",
        "log4js": "^2.5.3",
        "lusca": "^1.5.1",
        "mocha": "^3.5.3",
        "moment": "^2.22.2",
        "mongodb": "^2.2.34",
        "mongoose": "^5.4.20",
        "multer": "^1.3.0",
        "mysql": "^2.15.0",
        "mysql2": "^1.5.2",
        "node-schedule": "^1.3.0",
        "nodemailer": "^4.5.0",
        "passport": "0.4.0",
        "passport-jwt": "^3.0.1",
        "passport-local": "^1.0.0",
        "passport-oauth": "^1.0.0",
        "pdfmake": "^0.1.39",
        "pug": "^2.0.3",
        "random-token-generator": "^1.0.2",
        "sequelize": "^4.33.4",
        "sharp": "^0.20.5",
        "socket.io": "^2.0.4",
        "swagger-jsdoc": "^1.3.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-core": "^6.0.20",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.0.16",
        "babel-preset-env": "^1.6.0",
        "babel-preset-stage-0": "^6.0.15",
        "chai": "^4.1.1",
        "mocha": "^3.5.0",
        "nodemon": "^1.15.1",
        "supertest": "^3.0.0"
    },
    "env": {
        "jasmine": true,
        "mocha": true,
        "node": true
    },
    "plugins": [
        "chai-friendly"
    ]
}
